{
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["babel-plugin-styled-components",{
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }]
  ],
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "@styled-jsx/plugin-sass"
          ]
        }
      }
    ]
  ]
}
